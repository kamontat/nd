    "@strictsoftware/typedoc-plugin-monorepo": "0.2.1",
    "@types/async": "3.0.1",
    "@types/bcryptjs": "2.4.2",
    "@types/chai": "4.2.0",
    "@types/cheerio": "0.22.13",
    "@types/debug": "4.1.5",
    "@types/expect": "1.20.4",
    "@types/jsonwebtoken": "8.3.4",
    "@types/mock-fs": "3.6.30",
    "@types/ms": "0.7.30",
    "@types/mustache": "0.8.32",
    "@types/node": "10",
