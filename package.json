    "dev:admin": "make compile mode=development quite=true && node dist/nd-admin.js",
    "prod": "make compile mode=production && node dist/nd.min.js",
    "start": "node dist/nd.js",
    "start:prod": "node dist/nd.min.js",
    "build": "make build mode=production",
    "test": "make test mode=test",
    "test:debug": "export TS_NODE_DEBUG=true && yarn test",
    "release:lib": "./scripts/release_lib.sh",
    "clean": "make clean",
    "clean:all": "make clean all=true",
    "loc": "make loc type=sum && make loc type=lib"
