    "closure-webpack-plugin": "2.0.1",
    "css-loader": "3.2.0",
    "eslint": "6.4.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-loader": "3.0.2",
    "eslint-plugin-prettier": "3.1.1",
    "gh-pages": "2.1.1",
    "google-closure-compiler": "20190729.0.0",
    "html-loader": "0.5.5",
