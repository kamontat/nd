{
  "name": "nd",
  "version": "3.0.0-alpha.3",
  "description": "download manager for novel",
  "main": "index.ts",
  "bin": "dist/nd.min.js",
  "author": "kc nd-admin",
  "private": true,
  "scripts": {
    "build:lib": "make lib",
    "build:prod": "make build mode=production",
    "compile": "make compile mode=development",
    "compile:prod": "make compile mode=production",
    "dev": "make compile mode=development quite=true && node dist/nd.js",
    "start": "make compile mode=production && node dist/nd.min.js",
    "test": "make test mode=test",
    "test:debug": "export TS_NODE_DEBUG=true && yarn test",
    "release:lib": "./scripts/release_lib.sh",
    "clean": "make clean"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "chalk": "2.4.2",
    "debug": "4.1.1",
    "jsonwebtoken": "8.5.1",
    "nd-commandline-interpreter": "./modules/nd-commandline-interpreter",
    "nd-config": "./modules/nd-config",
    "nd-error": "./modules/nd-error",
    "nd-helper": "./modules/nd-helper",
    "nd-logger": "./modules/nd-logger",
    "nd-security": "./modules/nd-security",
    "tabtab": "3.0.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/chai": "4.1.7",
    "@types/debug": "4.1.4",
    "@types/expect": "1.20.4",
    "@types/jsonwebtoken": "8.3.2",
    "@types/mocha": "5.2.7",
    "@types/node": "10",
    "@types/tabtab": "3.0.0",
    "chai": "4.2.0",
    "closure-webpack-plugin": "2.0.1",
    "google-closure-compiler": "20190528.1.0",
    "mocha": "6.1.4",
    "mochawesome": "^4.0.1",
    "nyc": "^14.1.1",
    "ts-loader": "6.0.3",
    "ts-node": "8.3.0",
    "tslint": "5.17.0",
    "tslint-loader": "3.5.4",
    "typescript": "3.5.2",
    "uglifyjs-webpack-plugin": "2.1.3",
    "webpack": "4.35.0",
    "webpack-cli": "3.3.4",
    "webpack-node-externals": "1.7.2",
    "webpack-visualizer-plugin": "./modules/webpack-visualizer"
  },
  "pkg": {
    "assets": [
      "package.json"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.spec.ts",
      "reports/**/*",
      "dist/**/*",
      "coverage/**/*",
      "modules/webpack-visualizer/**/*"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true
  },
  "engines": {
    "node": ">=10.0.0 <11.0.0",
    "npm": ">=6.0.0 <7.0.0 ",
    "yarn": ">=1.16.0 <1.17.0"
  }
}